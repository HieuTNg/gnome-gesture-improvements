{
  "name": "gnome-gestures",
  "version": "1.0.0",
  "description": "Touchpad Gesture Improvements",
  "main": "build/extension.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:extension": "eslint extension scripts",
    "lint:package": "eslint build --fix",
    "transpile:watch": "tsc --watch",
    "transpile": "npm run lint:extension && tsc",
    "transpilenoeslint": "tsc -p tsconfig.json",
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && npm run transpile",
    "pack": "npm run build && make pack",
    "update": "npm run pack && make update"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.gnome.org/harshadgavali/gnome-gestures.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint__js": "^8.42.3",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.14.10",
    "@types/yargs": "^17.0.32",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "dependencies": {
    "@girs/gjs": "^4.0.0-beta.6",
    "@girs/gnome-shell": "^46.0.0-beta8",
    "glob": "^11.0.0",
    "path": "^0.12.7",
    "yargs": "^17.7.2"
  }
}
